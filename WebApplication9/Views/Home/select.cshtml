@model IEnumerable<WebApplication9.Models.State>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StateName)
        </th>
        <th>
            Election Type
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ElectionDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResultDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Seats)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StateName)
            </td>
            <td>
                @ViewBag.message
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ElectionDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ResultDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Seats)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.StateId }) |
                @Html.ActionLink("Selected", "Selected", new { id = item.StateId })
            </td>
        </tr>
    }

</table>
